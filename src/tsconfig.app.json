{
  "extends": "../tsconfig.json",
  "compilerOptions": {
    "outDir": "../out-tsc/app",
    "types": [],
    "paths": {
      "@shared/*": ["src/app/shared/*"],
      "@modal/*": ["src/app/modal/*"],
      "@rootStore/*": ["src/app/store/*"],
      "@env/*": ["src/environments/*"],
      "core-js/es7/reflect": [
        "./node_modules/core-js/proposals/reflect-metadata"
      ],
      "core-js/es6/*": [
        "./node_modules/core-js/es/*"
      ]
    }
  },
  "exclude": [
    "test.ts",
    "**/*.spec.ts"
  ]
}
